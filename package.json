{
  "name": "ai-arena-backend",
  "version": "1.0.0",
  "description": "Test your strategy and programming skills in the AI Arena.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint:check && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:check": "yarn run prettier --check src",
    "prettier:fix": "yarn run prettier --write src",
    "eslint:check": "yarn run eslint --report-unused-disable-directives src",
    "eslint:fix": "yarn run eslint:check --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t ghcr.io/a-gondolkodas-orome/ai-arena-backend .",
    "docker:run": "docker run -p 3000:3000 -d ai-arena-backend",
    "docker:publish": "yarn run docker:build && docker push ghcr.io/a-gondolkodas-orome/ai-arena-backend",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist '*.tsbuildinfo' .eslintcache",
    "rebuild": "yarn run clean && yarn run build",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leanil/ai-arena-backend"
  },
  "author": "Andr√°s Leitereg",
  "license": "UNLICENSED",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^11.0.9",
    "@loopback/authentication-jwt": "^0.15.9",
    "@loopback/boot": "^7.0.9",
    "@loopback/core": "^6.1.6",
    "@loopback/filter": "^5.0.8",
    "@loopback/graphql": "^0.11.9",
    "@loopback/metrics": "^0.14.9",
    "@loopback/repository": "^7.0.9",
    "@loopback/rest": "^14.0.9",
    "@loopback/rest-explorer": "^7.0.9",
    "@loopback/security": "^0.11.9",
    "@redis/client": "^1.6.0",
    "apollo-server-errors": "^3.3.1",
    "base64-js": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "commander": "^13.0.0",
    "dataloader": "^2.2.3",
    "fp-ts": "^2.16.9",
    "graphql": "^16.10.0",
    "graphql-scalars": "^1.24.0",
    "io-ts": "^2.2.22",
    "jsonwebtoken": "9",
    "loopback-connector-mongodb": "^7.0.0-alpha.1",
    "md5": "^2.3.0",
    "mongodb": "^6.12.0",
    "multer": "^1.4.5-lts.1",
    "tslib": "^2.8.1",
    "type-graphql": "^2.0.0-rc.2"
  },
  "peerDependencies": {
    "class-validator": "^0.13.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@loopback/build": "^11.0.8",
    "@loopback/testlab": "^7.0.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/k6": "^0.54.2",
    "@types/md5": "^2.3.5",
    "@types/mocha": "^10.0.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.6",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-mocha": "^10.5.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "private": true,
  "packageManager": "yarn@3.2.3"
}
