{
  "name": "ai-arena-backend",
  "version": "1.0.0",
  "description": "Test your strategy and programming skills in the AI Arena.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint:check && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:check": "yarn run prettier --check src",
    "prettier:fix": "yarn run prettier --write src",
    "eslint:check": "yarn run eslint --report-unused-disable-directives src",
    "eslint:fix": "yarn run eslint:check --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t ai-arena-backend .",
    "docker:run": "docker run -p 3000:3000 -d ai-arena-backend",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist '*.tsbuildinfo' .eslintcache",
    "rebuild": "yarn run clean && yarn run build",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leanil/ai-arena-backend"
  },
  "author": "Andr√°s Leitereg",
  "license": "UNLICENSED",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^9.0.6",
    "@loopback/authentication-jwt": "^0.12.6",
    "@loopback/boot": "^5.0.6",
    "@loopback/core": "^4.0.6",
    "@loopback/filter": "^3.0.6",
    "@loopback/graphql": "^0.8.6",
    "@loopback/repository": "^5.1.1",
    "@loopback/rest": "^12.0.6",
    "@loopback/rest-explorer": "^5.0.6",
    "@loopback/security": "^0.8.6",
    "apollo-server-errors": "^3.3.1",
    "base64-js": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "fp-ts": "^2.12.3",
    "graphql": "^15.3.0",
    "io-ts": "^2.2.18",
    "jsonwebtoken": "9",
    "loopback-connector-mongodb": "^6.2.0",
    "md5": "^2.3.0",
    "mongodb": "^4.10.0",
    "multer": "^1.4.5-lts.1",
    "tslib": "^2.0.0",
    "type-graphql": "^1.1.1"
  },
  "peerDependencies": {
    "class-validator": "^0.13.2"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.6",
    "@loopback/eslint-config": "^13.0.10",
    "@loopback/testlab": "^5.0.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/md5": "^2.3.2",
    "@types/mocha": "^10.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.18.34",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-mocha": "^10.1.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.9.4"
  },
  "private": true,
  "packageManager": "yarn@3.2.3"
}
